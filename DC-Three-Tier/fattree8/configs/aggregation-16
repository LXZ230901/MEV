router bgp 16
    router-id 70.0.16.0
    cluster-id 70.0.16.0
    address-family ipv4 unicast
        neighbor 10.0.128.0
           remote-as 0
           address-family ipv4 unicast
              send-community extended
           address-family l2vpn evpn
              send-community extended
        neighbor 10.0.136.0
           remote-as 1
           address-family ipv4 unicast
              send-community extended
           address-family l2vpn evpn
              send-community extended
        neighbor 10.0.144.0
           remote-as 2
